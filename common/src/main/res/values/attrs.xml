<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="entrySummaries" format="reference" />

    <declare-styleable name="IjkListPreference">
        <attr name="entrySummaries" />
    </declare-styleable>
    <declare-styleable name="TextViewWithTTF">
        <attr name="ttf_name" format="string" />
    </declare-styleable>
    <!-- 倒影控件属性 -->
    <declare-styleable name="reflectItemView">
        <attr name="isReflect" format="boolean" />
        <attr name="isShape" format="boolean" />
        <attr name="reflect_height" format="dimension" />
        <attr name="allRadius" format="dimension" />
        <attr name="refle_spacing" format="dimension" />
    </declare-styleable>

    <!-- 顶层控件属性 -->
    <declare-styleable name="MainUpView">
        <attr name="isTvScreen" format="boolean" />
        <attr name="isDrawUpRect" format="boolean" />
        <attr name="upImageRes" format="reference" />
        <attr name="shadowImageRes" format="reference" />
    </declare-styleable>
<!--    <declare-styleable name="ScrollText">-->
<!--        <attr name="clickEnable" format="boolean" />-->
<!--        <attr name="isHorizontal" format="boolean" />-->
<!--        <attr name="speed" format="integer" />-->
<!--        <attr name="text" format="string" />-->
<!--        <attr name="textColor" format="color" />-->
<!--        <attr name="textSize" format="dimension" />-->
<!--        <attr name="times" format="integer" />-->
<!--    </declare-styleable>-->

    <declare-styleable name="WaveLoadingView">
        <attr name="wlv_borderWidth" format="dimension" />
        <attr name="wlv_borderColor" format="color" />
        <attr name="wlv_progressValue" format="integer" />
        <attr name="wlv_shapeType" format="enum">
            <enum name="circle" value="0" />
            <enum name="square" value="1" />
        </attr>
        <attr name="wlv_waveColor" format="color" />
        <attr name="wlv_waveAmplitude" format="float" />
        <attr name="wlv_titleTopSize" format="dimension" />
        <attr name="wlv_titleCenterSize" format="dimension" />
        <attr name="wlv_titleBottomSize" format="dimension" />
        <attr name="wlv_titleTopColor" format="color" />
        <attr name="wlv_titleCenterColor" format="color" />
        <attr name="wlv_titleBottomColor" format="color" />
        <attr name="wlv_titleTop" format="string" />
        <attr name="wlv_titleCenter" format="string" />
        <attr name="wlv_titleBottom" format="string" />
    </declare-styleable>

    <declare-styleable name="CountDownProgress">
        <!-- 默认圆实心的颜色 -->
        <attr name="cdp_circle_solid_color" format="color|reference" />
        <!-- 默认圆边框的颜色 -->
        <attr name="cdp_circle_stroke_color" format="color|reference" />
        <!-- 默认圆边框的宽度 -->
        <attr name="cdp_circle_stroke_width" format="dimension" />
        <!-- 默认圆的半径 -->
        <!--<attr name="cdp_circle_radius" format="dimension" />-->
        <!-- 进度条的颜色 -->
        <attr name="cdp_progress_color" format="color|reference" />
        <!-- 进度条的宽度 -->
        <attr name="cdp_progress_width" format="dimension" />
        <!-- 文字的颜色 -->
        <attr name="cdp_text_color" format="color|reference" />
        <!-- 文字的字体大小 -->
        <attr name="cdp_text_size" format="dimension" />
        <attr name="cdp_text" format="string|reference" />
        <attr name="cdp_count_down_time" format="integer|reference" />
        <attr name="cdp_start_degree" format="float|reference" />
    </declare-styleable>
    <declare-styleable name="RoundImpl">
        <attr name="bottomRightRadius" format="dimension" />
        <attr name="bottomLeftRadius" format="dimension" />
        <attr name="radius"  format="dimension" />
        <attr name="topRightRadius" format="dimension" />
        <attr name="topLeftRadius" format="dimension" />

    </declare-styleable>

    <declare-styleable name="MagicTextView">
        <attr name="innerShadowColor" format="color" />
        <attr name="innerShadowRadius" format="float" />
        <attr name="innerShadowDx" format="float" />
        <attr name="innerShadowDy" format="float" />
        <attr name="outerShadowColor" format="color" />
        <attr name="outerShadowRadius" format="float" />
        <attr name="outerShadowDx" format="float" />
        <attr name="outerShadowDy" format="float" />
        <attr name="typeface" format="string" />
        <attr name="foreground" format="reference|color" />
        <!--<attr name="background" format="reference|color"/>-->
        <attr name="strokeWidth1" format="float" />
        <attr name="strokeMiter" format="float" />
        <attr name="strokeColor" format="color" />
        <attr name="strokeJoinStyle">
            <enum name="miter" value="0" />
            <enum name="bevel" value="1" />
            <enum name="round" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MarqueeTextView">
        <attr name="scroll_interval" format="integer" />
        <attr name="scroll_first_delay" format="integer" />
        <attr name="scroll_mode">
            <enum name="mode_forever" value="100" />
            <enum name="mode_once" value="101" />
        </attr>
    </declare-styleable>

    <declare-styleable name="BoxedVertical">
        <attr name="points" format="integer" />
        <attr name="max" format="integer" />
        <attr name="min" format="integer" />
        <attr name="defaultValue" format="integer" />
        <attr name="textBottomPadding" format="integer" />
        <attr name="cornerRadius1" format="integer" />
        <attr name="step" format="integer" />
        <attr name="defaultImage" format="reference" />
        <attr name="minImage" format="reference" />
        <attr name="maxImage" format="reference" />
        <attr name="progressColor" format="color" />
        <attr name="backgroundColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="textColor" format="color" />
        <attr name="clockwise" format="boolean" />
        <attr name="enabled" format="boolean" />
        <attr name="touchDisabled" format="boolean" />
        <attr name="textEnabled" format="boolean" />
        <attr name="imageEnabled" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TvItemArrowLayout">
        <attr name="select_setup" format="boolean" />
    </declare-styleable>

    <declare-styleable name="PickerView">
        <attr name="line_Color" format="color|reference" />
        <attr name="default_textSize" format="dimension" />
        <attr name="default_textColor" format="color|reference" />
        <attr name="checked_textColor" format="color|reference" />
        <attr name="is_tone" format="boolean" />
    </declare-styleable>

    <!-- CircleProgressView 使用的属性  -->
    <declare-styleable name="CircleProgressView">
        <attr name="progress_circle_radius" format="dimension" />
        <attr name="progress_unreach_color" format="color" />
        <attr name="progress_unreach_height" format="dimension" />
        <attr name="progress_reach_color" format="color" />
        <attr name="progress_reach_height" format="dimension" />
        <attr name="progress_text_color" format="color" />
        <attr name="progress_text_size" format="dimension" />
        <attr name="progress_text_offset" format="dimension" />
    </declare-styleable>

    <declare-styleable name="TitleShowItemLayout">
        <attr name="has_warning" format="boolean" />
        <attr name="idle_mode" format="boolean" />
        <attr name="other_mode" format="boolean" />
    </declare-styleable>

    <declare-styleable name="RcvSlider">
        <attr name="main_line_color" format="color|reference" />
        <attr name="slider_color" format="color|reference" />
        <attr name="slider_width" format="integer" />
        <attr name="slider_height" format="integer" />
        <attr name="main_line_length" format="integer" />
    </declare-styleable>

    <declare-styleable name="ButtonBarContainerTheme">
        <attr name="buttonBarStyle" format="reference" />
        <attr name="buttonBarButtonStyle" format="reference" />
    </declare-styleable>

    <declare-styleable name="WifiEncryptionState">
        <attr name="state_encrypted" format="boolean" />
    </declare-styleable>
</resources>